{
  "name": "data-structures",
  "version": "0.0.1",
  "scripts": {
    "pre-commit": "npm-run-all jshint jscs tests",
    "pre-push": "npm-run-all jshint jscs coverage",
    "tests": "mocha --ui bdd -R spec -t 5000 --recursive tests",
    "coverage": "node node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 --recursive tests",
    "jshint": "node node_modules/jshint/bin/jshint index.js tests linked-lists --config .jshintrc",
    "jscs": "node node_modules/jscs/bin/jscs index.js tests linked-lists --config .jscsrc"
  },
  "dependencies": {
    "lodash": "^4.15.0"
  },
  "devDependencies": {
    "ghooks": "^1.3.2",
    "istanbul": "^0.4.5",
    "jscs": "^3.0.7",
    "jshint": "^2.9.3",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.0.0",
    "should": "^11.1.0"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  }
}
